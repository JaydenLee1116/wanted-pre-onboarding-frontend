{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 110,
  "proseWrap": "preserve",
  "quoteProps": "consistent",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "requirePragma": false,
  "insertPragma": false,
  "plugins": ["prettier-plugin-tailwindcss"]
}
